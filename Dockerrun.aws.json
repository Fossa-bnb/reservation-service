{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "reservation-app",
      "image": "tremarkley/reservation-service",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3002
        }
      ], 
      "links": [
        "db"
      ]
    },
    {
      "name": "db",
      "image": "tremarkley/reservationservice_database",
      "essential": true,
      "memory": 128
    }
  ]
}